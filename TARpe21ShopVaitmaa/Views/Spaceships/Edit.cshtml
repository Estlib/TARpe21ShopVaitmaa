@model TARpe21ShopVaitmaa.Models.Spaceship.SpaceshipEditViewModel
<h1>Add</h1>
<div>
    <form method="post" enctype="multipart/form-data">
        <input asp-for="Id" type="hidden" />
        <input asp-for="CreatedAt" type="hidden" />
        <input asp-for="ModifiedAt" type="hidden" />

        <div class="form-group row">
            <label asp-for="Name" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="Name" class="form-control"/>
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="Type" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="Type" class="form-control"/>
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="Description" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="Description" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="PassengerCount" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="PassengerCount" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="CrewCount" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="CrewCount" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="CargoWeight" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="CargoWeight" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="MaxSpeedInVaccuum" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="MaxSpeedInVaccuum" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="BuiltAtDate" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="BuiltAtDate" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="MaidenLaunch" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="MaidenLaunch" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="Manufacturer" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="Manufacturer" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="IsSpaceshipPreviouslyOwned" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="IsSpaceshipPreviouslyOwned" class="form-check-input" />
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="FullTripsCount" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="FullTripsCount" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="EnginePower" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="EnginePower" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="FuelConsumptionPerDay" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="FuelConsumptionPerDay" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="MaintenanceCount" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="MaintenanceCount" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="LastMaintenance" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="LastMaintenance" class="form-control" />
            </div>
        </div>

    </form>
</div>