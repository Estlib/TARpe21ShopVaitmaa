            <label asp-for="SqMPrice" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.SqMPrice)
        </div>

        <div class="form-group-row">
            <label asp-for="BuildDate" class="col-sm-2 col-form-label"></label>
